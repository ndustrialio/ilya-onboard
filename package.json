{
  "name": "ndustrial-react-starter",
  "version": "0.1.0",
  "description": "ndustrial.io react starter application",
  "main": "app.js",
  "scripts": {
    "build": "npm run prod-build",
    "clean": "rimraf dist",
    "compile": "webpack",
    "coverage": "NODE_ENV=test nyc mocha -R spec 'src/**/*.spec.js'",
    "lint": "npm run lint:js; npm run lint:styles",
    "lint:js": "eslint '+(src|test)/**/*.js'",
    "lint:styles": "stylelint './src/**/*.scss';",
    "precommit": "pretty-quick --staged",
    "prod": "NODE_ENV=production nodemon src/server.js",
    "prod-build": "NODE_ENV=production webpack --config webpack.production.config.js",
    "prod-start": "NODE_ENV=production node src/server.js",
    "report": "nyc report --reporter=lcov",
    "serve": "npm run prod-start",
    "start": "webpack-dev-server --color",
    "test": "NODE_ENV=test mocha -R spec 'src/**/*.spec.js'",
    "test:dev": "npm run test -- --watch",
    "test:inspect": "npm run test -- --inspect-brk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndustrialio/react-starter.git"
  },
  "keywords": [
    "react",
    "webpack",
    "nodejs",
    "node"
  ],
  "author": "ndustrial.io <dev@ndustrial.io> (ndustrial.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ndustrialio/react-starter/issues"
  },
  "homepage": "https://github.com/ndustrialio/react-starter#readme",
  "devDependencies": {
    "@ndustrial/stylelint-config-ndustrial": "^1.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^4.19.1",
    "eslint-config-ndustrial": "^1.1.0",
